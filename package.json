{
  "name": "vue3_vite",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "serve": "vite",
    "dev": "vite --mode development",
    "test": "vite --mode test",
    "staging": "vite --mode staging",
    "production": "vite --mode production",
    "devEnv": "cross-env NODE_ENV=development vite --mode development",
    "testEnv": "cross-env NODE_ENV=test vite --mode test",
    "stagingEnv": "cross-env NODE_ENV=staging vite --mode staging",
    "productionEnv": "cross-env NODE_ENV=production vite --mode production",
    "build": "vue-tsc && vite build",
    "build:dev": "vue-tsc && vite build --mode development",
    "build:test": "vue-tsc && vite build --mode test",
    "build:staging": "vue-tsc && vite build --mode staging",
    "build:production": "vue-tsc && vite build --mode production",
    "build:devEnv": "vue-tsc && cross-env NODE_ENV=development vite build --mode development",
    "build:testEnv": "vue-tsc && cross-env NODE_ENV=test vite build --mode test",
    "build:stagingEnv": "vue-tsc && cross-env NODE_ENV=staging vite build --mode staging",
    "build:productionEnv": "vue-tsc && cross-env NODE_ENV=production vite build --mode production",
    "preview": "vite preview",
    "decCss": "tailwind build ./src/tailwindcss/style.css -o ./css/style.css",
    "lint:eslint": "eslint --cache --max-warnings 0 {src,mock}/**/*.{vue,ts,tsx} --fix",
    "lint:prettier": "prettier --write **/*.{ts,js,json,tsx,css,less,scss,vue,html,md}",
    "lint:style": "stylelint src/**/*.vue --fix --custom-syntax postcss-less",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install",
    "vitest": "vitest"
